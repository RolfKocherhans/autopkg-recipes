<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>ftech dowload !</string>
	<key>Description</key>
	<string>Downloads the latest version of fetch</string>
	<key>Identifier</key>
	<string>com.github.rolfkocherhans.download.Fetch.test</string>
	<key>Input</key>
	<dict>
		<key>DOWNLOAD_URL</key>
		<string>http://getfetch8.com</string>
		<key>NAME</key>
		<string>Fetch</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.5.0</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>Fetch_*.dmg</string>
				<key>url</key>
				<string>http://fetchsoftworks.com</string>
			</dict>
			<key>Processor</key>
			<string>URLTextSearcher</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>file_content</key>
				<string>AutoPkg Variables:
				DOWNLOAD_URL=%DOWNLOAD_URL%
				Name=%NAME%
				match=%match%
				url=%DOWNLOAD_URL%%match%</string>
				<key>file_path</key>
				<string>%RECIPE_CACHE_DIR%/AutoPkg Variables.txt</string>
			</dict>
			<key>Processor</key>
			<string>FileCreator</string>
		</dict>
	</array>
</dict>
</plist>
